{
  "name": "@drawilet/nextjs-i18n",
  "version": "0.0.11",
  "description": "Simplify Next.js internationalization with our client-side package. Seamlessly integrate and access translated content in your project with ease.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Drawilet/nextjs-i18n.git"
  },
  "author": "Drawilet <drawiletdev@gmail.com>",
  "keywords": [
    "i18n",
    "next",
    "nextjs",
    "internationalization"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "bugs": {
    "url": "https://github.com/Drawilet/nextjs-i18n/issues"
  },
  "homepage": "https://github.com/Drawilet/nextjs-i18n#readme"
}
